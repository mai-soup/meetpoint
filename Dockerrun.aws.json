{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "meetpoint-server",
      "image": "MY_ECR_REPOSITORY_SERVER",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 5000,
          "containerPort": 5000
        }
      ]
    },
    {
      "name": "meetpoint-client",
      "image": "MY_ECR_REPOSITORY_CLIENT",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 3000,
          "containerPort": 3000
        }
      ]
    }
  ]
}
